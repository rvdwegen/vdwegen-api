{
    "scriptFile": "../entry.psm1",
    "entryPoint": "Receive-HttpTrigger",
    "bindings": [
      {
        "authLevel": "anonymous",
        "type": "httpTrigger",
        "direction": "in",
        "name": "Request",
        "methods": ["get", "post"],
        "route": "{URLslug}"
      },
      {
        "type": "http",
        "direction": "out",
        "name": "Response"
      }
    ]
  }